doctype html
html(lang="en", ng-app="glowingAdventure")
    head
        title Glowing Adventure
        link(href="/libs/bootstrap/dist/css/bootstrap.min.css", rel="stylesheet")
        link(href="/css/app.css", rel="stylesheet")
    body
        .navbar.navbar-inverse.navbar-fixed-top
            .container-fluid
                .navbar-header
                    button(type="button", data-toggle="collapse", data-target=".navbar-collapse").navbar-toggle
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a(href="#").navbar-brand Visalg
                .navbar-collapse.collapse
                    ul.nav.navbar-nav.navbar-right
                        li: a(href="#") Dashboard
                        li: a(href="#") Help
                    form.navbar-form.navbar-right
                        input(type="text", placeholder="Search...").form-control

        .container-fluid
            .row
                .col-sm-3.col-md-2.sidebar
                    ul.nav.nav-sidebar
                        li: a(href="/").active Overview
                        li: a(href="/brute-force") Brute Force
                        li: a(href="/kmp") Knuth-Morris-Pratt
                        li: a(href="/boyer-moore") Boyer-Moore
        div(ng-view).col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-2.main
            h1.page-header Visalg

        script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js")
        //-script(src="libs/bootstrap/dist/js/bootstrap.min.js")
        script(src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.17/angular.js")
        script(src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.17/angular-route.js")
        script(src="js/app.js")
        script(src="js/stringBruteForceCtrl.js")
